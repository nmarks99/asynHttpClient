record(lso, "$(P)set_action_url:getj") {
    field(DOL, {const: "http://$(HOST)/action?action_name=getj"})
    field(OUT, "$(P)$(R)FullURL.VAL$ PP")
    field(SIZV, 2048)
}

record(bo, "$(P)URModule:toggle_gripper") {
    field(ZNAM, "Open")
    field(ONAM, "Close")
    field(FLNK, "$(P)run_toggle_gripper.PROC")
}

record(luascript, "$(P)run_toggle_gripper") {
    field(INPA, "$(P)URModule:toggle_gripper.VAL")
    field(CODE, "@ur_module.lua get_url_toggle_gripper({prefix='$(P)',host='$(HOST)'})")
    field(SYNC, "Sync")
    field(OUT, "$(P)$(R)FullURL.VAL$ PP")
    field(FLNK, "$(P)$(R)Post.PROC")
}
